<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">
 
    <insert id="registerMember" parameterType="hashmap">
    
      INSERT INTO member(
      	  id,
		  name,
		  age,
		  gender,
		  address,
		  password
      ) 
      VALUES( 
      		  #{userId}, 
      		  #{userName}, 
      		  #{age}, 
      		  #{gender}, 
      		  #{addr},
      		  #{passWord}
      		  )
       
    </insert>
 
</mapper>